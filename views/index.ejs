<!DOCTYPE html>
<html lang="en" ng-app="quizRT">
<head>
  <meta charset="UTF-8">
  <title>QuizART</title>
  <meta name="description" content="">
	<!-- Mobile viewport optimized -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

  <script type="text/javascript" src='js/lib/jquery-2.1.4.js'></script>
<link href='https://fonts.googleapis.com/css?family=Bitter' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <link rel="stylesheet" href="css/lib/bootstrap.min.css">
  <link rel="stylesheet" href="css/lib/ngToast.min.css">
  <link rel="stylesheet" href="css/lib/ngToast-animations.min.css">
  <link rel="stylesheet" href="css/lib/c3.css">
  <link rel="stylesheet" href="css/style.css">
  <link rel="stylesheet" href="css/userProfile.css">
  <link rel="stylesheet" href="css/lib/rzslider.min.css">
  <link rel="stylesheet" ng-href="css/tournamentCreation.css">
  <link rel="stylesheet" ng-href="css/{{stylesheetName}}.css">
  <link rel="stylesheet" href="css/newTheme.css">
</head>

<body >
    <header>
      <div class="container">
        <div class="row">
    			<div class="col-xs-offset-1 col-lg-9 col-md-8 col-sm-6 col-xs-4 ">
    				QuizART
    			</div>
    			<div ng-if="loggedInUser" class="col-lg-2 col-md-3 col-sm-4 col-xs-6">
            <notification-data notification-count="notificationCount"></notification-data>
            <a ng-click="$emit('logout',loggedInUser)" class="btn login-btn glyphicon glyphicon-off">
    				</a>
    			</div>
          <div ng-if="!loggedInUser" class="col-xs-2">
    				<a ng-click="$emit('login')" class="btn logoff-btn glyphicon glyphicon-play-circle">
    				</a>
    			</div>
    		</div>
      </div>
    </header>

    <div class="view-container" ng-view>
    </div>
    <toast></toast>
    <footer ng-if="loggedInUser" id="footer-nav">
      <nav>
      	<div class="container">
      		<div class="row">
      			<div class="col-xs-3 nav">
      				<div>
      					<span  ng-click="redirectTo('userProfile')" class="glyph glyphicon glyphicon-user"></span>
      					<p>Profile</p>
      				</div>
      			</div>
      			<div class="col-xs-2 nav">
      				<div>
            		<img  ng-click="redirectTo('userTeams')" class="img-responsive" src='images/team_64x64.png' alt='Teams Img'>
      					<p>Teams</p>
      				</div>
      			</div>
      			<div class="col-xs-2 nav">
      				<div>
      					<span class="glyph glyphicon glyphicon-globe" ng-click="redirectTo('userTournaments')"></span>
      					<p>Tournaments</p>
      				</div>
      			</div>
      			<div class="col-xs-2 nav">
      				<div>
      					<span  ng-click="redirectTo('userSettings')" class="glyph glyphicon glyphicon-cog"></span>
      					<p>Settings</p>
      				</div>
      			</div>
            <div class="col-xs-3 nav">
      				<div>
      					<span  ng-click="redirectTo('badges')" class="glyph glyphicon glyphicon-briefcase"></span>
      					<p>Badges</p>
      				</div>

      			</div>
      		</div>
      	</div>
      </nav>
    </footer>

</body>
<script type="text/javascript" src='js/lib/socket-io-1.3.7.js'></script>
<script type="text/javascript" src='js/lib/jquery-2.1.4.js'></script>
<script type="text/javascript" src='js/lib/bootstrap.min.js'></script>
<script type="text/javascript" src='js/lib/angular.min.js'></script>
<script type="text/javascript" src='js/lib/angular-route.min.js'></script>
<script type="text/javascript" src="js/lib/angular-cookies.min.js"></script>
<script type="text/javascript" src="js/lib/angular-animate.min.js"></script>
<script type="text/javascript" src="js/lib/angular-sanitize.min.js"></script>
<script type="text/javascript" src="js/lib/ngToast.min.js"></script>
<script type="text/javascript" src="js/lib/ui-bootstrap-1.3.2.min"></script>
<script type="text/javascript" src="js/lib/rzslider.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.14.3/ui-bootstrap-tpls.js"></script>

<script type="text/javascript" src="js/lib/d3.min.js"></script>
<script type="text/javascript" src="js/lib/d3.tip.v0.6.3.js"></script>
<script type="text/javascript" src='js/routeProvider.js'></script>
<script type="text/javascript" src='js/controllers/quizPlayerController.js'></script>
<script type="text/javascript" src='js/controllers/tournamentPlayController.js'></script>
<script type="text/javascript" src='js/controllers/userProfileController.js'></script>
<!-- c3 graph library and a angular directive for that  -->
<script type="text/javascript" src="js/lib/c3.js"></script>
<script type="text/javascript" src="js/lib/angular_c3_simple.js"></script>
<script type="text/javascript" src='js/controllers/userTournamentsController.js'></script>
<script type="text/javascript" src='js/controllers/badgesController.js'></script>
<script type="text/javascript" src='js/controllers/userSettingsController.js'></script>
<script type="text/javascript" src='js/controllers/categoriesController.js'></script>
<script type="text/javascript" src='js/controllers/categoryController.js'></script>
<script type="text/javascript" src='js/controllers/topicController.js'></script>
<script type="text/javascript" src='js/controllers/authController.js'></script>
<script type="text/javascript" src='js/controllers/resultController.js'></script>
<script type="text/javascript" src='js/controllers/userCurrentGameStatChart.js'></script>
<script type="text/javascript" src='js/controllers/tournamentController.js'></script>
<script type="text/javascript" src='js/controllers/tournamentPlayerController.js'></script>
<script type="text/javascript" src='js/controllers/tournamentPlayController.js'></script>
<script type="text/javascript" src='js/controllers/halloffameController.js'></script>
<script type="text/javascript" src='js/controllers/tournamentCreationController.js'></script>
<script type="text/javascript" src='js/controllers/inviteFriendsController.js'></script>
<script type="text/javascript" src='js/controllers/notificationController.js'></script>
<script type="text/javascript" src='js/notificationDirective.js'></script>
</html>
